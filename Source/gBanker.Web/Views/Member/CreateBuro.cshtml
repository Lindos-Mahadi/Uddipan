                data: { nationalId: nationalId },
                dataType: 'json',
                async: true,
                success: function (cnt_status) {
                    $("#loading").hide();
                    if (cnt_status == 'Yes') {
                        $("span[data-valmsg-for='NationalID']").text('National Id already exists');
                        $("#NationalID").val('');
                        $("#NationalID").focus();
                    }
                },
