        //    var str = file;
        //    var l = str.split("/")
        //    var a = document.getElementById("thumbnil").innerHTML = l[l.length - 1];
        //    return a;
        //}


        $("#btnSave").click(function (e) {
            e.preventDefault();
            var _currentForm = $(this).closest('form');
            if (_currentForm.valid()) {
                $("#loading").show();
                var file = "";
                var fileFingerThumb = "";

                var files = document.getElementById("thumbnil").src;
                var arr = files.split("/");
                var pic = arr[arr.length - 1];
                if (pic == "blank-headshot.jpg") {
                    file = null;
                } else {
                    file = files;
                }
                var base64image = file;


                var fileFingerThumbs = document.getElementById("FingerThumb").src;
                var arr = fileFingerThumbs.split("/");
                var pics = arr[arr.length - 1];
                if (pics == "blank-headshot.jpg") {
                    fileFingerThumb = null;
                } else {
                    fileFingerThumb = fileFingerThumbs;
                }
                var base64imageFingerThumb = fileFingerThumb;



                $('#MemProfile').ajaxSubmit({
                    type: "POST",
                    target: '#thumbnil',
                    data: { MemCode: '', base64image: base64image, base64imageFingerThumb: base64imageFingerThumb },
                    dataType: 'json',
                    processData: false,
                    contentType: false,
                    // resetForm: true,
                    success: function (data) {
                        $("#loading").hide();
                        if (data.data.MemberID > 0) {
                            $("#thumbnil").attr("src", "../../Images/blank-headshot.jpg");
                            $("#FingerThumb").attr("src", "../../Images/blank-headshot.jpg");
                            //if(data.data.MemberCode != ''){
                            //alert('success');

                            $("#dialog-message").html('<p><span class="ui-icon ui-icon-circle-check" style="float:left; margin:0 7px 50px 0;"></span>Member Saved Successfully, Member Code is ' + data.data.MemberCode + '.</p>');

                            $("#FirstName").val('');
                            $("#MiddleName").val('');
                            $("#LastName").val('');
                            $("#FatherName").val('');
                            $("#MotherName").val('');
                            $("#Spouse").val('');
                            $("#CoApplicantName").val('');
                            //}
                            //else {
                            //    $( "#dialog-message" ).html('<p>Fail to Save.</p>');
                            //}
                            $("#CountryID").val('18');
                            $("#dialog-message").dialog({
                                modal: true,
                                buttons: {
                                    Ok: function () {
                                        $(this).dialog("close");
                                    }
                                }
                            });
                        }
                        else {
                            GetReverseMemberCode($("#OfficeID").val(), $("#GroupID").val());
                            $("#dialog-message").html('<p><span class="ui-icon ui-icon-circle-check" style="float:left; margin:0 7px 50px 0;"></span>Error, Fail to save member information.</p>');
                            $("#CountryID").val('18');
                            $("#dialog-message").dialog({
                                modal: true,
                                buttons: {
                                    Ok: function () {
                                        $(this).dialog("close");
                                    }
                                }
                            });
                        }
                    },
                    error: function (err) {
                        $("#loading").hide();
                        $("#CountryID").val('18');
                        $("#dialog-message").html('<p>Error, Fail to save member information.</p>');
                        $("#dialog-message").dialog({
                            modal: true,
                            buttons: {
                                Ok: function () {
                                    $(this).dialog("close");
                                }
                            }
                        });
                    }
                });


            }

            /////////////////


        });
    });

