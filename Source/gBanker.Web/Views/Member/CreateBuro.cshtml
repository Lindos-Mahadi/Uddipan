                    $("#loading").hide();
                    $("span[data-valmsg-for='NationalID']").text(request.statusText + "/" + request.statusText + "/" + error);
                    //alert(request.statusText + "/" + request.statusText + "/" + error);
                    $("#NationalID").focus();
                }
            });

            //}
            //else {
            //    ///alert('National Id cannot be blank or less than 10 characters.');
            //    $("#NationalID").focus();
            //}
        });

        $("#PhoneNo").blur(function (e) {
            var PhoneNo = $("#PhoneNo").val();

            $("#loading").show();
            $.ajax({
                type: "GET",
                contentType: "application/json; charset=utf-8",
                url: '@Url.Action("CheckPhoneNo", "Member", "http")',
                data: { PhoneNo: PhoneNo },
                dataType: 'json',
                async: true,
                success: function (cnt_status) {
                    $("#loading").hide();
                    if (cnt_status == 'Yes') {
                        alert('Phone No already exists');
                        $("#PhoneNo").val('');
                        $("#PhoneNo").focus();
                    }
                },
                error: function (request, status, error) {
                    $("#loading").hide();
                    alert(request.statusText + "/" + request.statusText + "/" + error);
                    $("#PhoneNo").focus();
                }
            });

        });

